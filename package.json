{
    "name": "react-avant",
    "version": "0.0.1",
    "description": "Commonly used React components",
    "author": "Hunmin Park",
    "files": [
        "lib"
    ],
    "peerDependencies": {
        "@fortawesome/fontawesome-svg-core": "1.x",
        "luminous-lightbox": "2.x",
        "prismjs": "1.x",
        "react": "17.x",
        "react-dom": "17.x",
        "react-markdown": "7.x",
        "remark-gfm": "3.x"
    },
    "devDependencies": {
        "@storybook/react": "^6.4.14",
        "@storybook/client-api": "^6.4.14",
        "@storybook/builder-webpack5": "^6.4.14",
        "@storybook/manager-webpack5": "^6.4.14",
        "@storybook/addon-essentials": "^6.4.14",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@types/node": "^17.0.10",
        "@types/prismjs": "^1.16.6",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@typescript-eslint/eslint-plugin": "^5.10.0",
        "@typescript-eslint/parser": "^5.10.0",
        "clean-webpack-plugin": "^4.0.0",
        "css-loader": "^6.5.1",
        "eslint": "^8.7.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsdoc": "^37.6.3",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.2.2",
        "mini-css-extract-plugin": "^2.5.2",
        "npm-run-all": "^4.1.5",
        "sass": "^1.49.0",
        "sass-loader": "^12.4.0",
        "ts-loader": "^9.2.6",
        "ttypescript": "^1.5.13",
        "typed-scss-modules": "^4.1.2",
        "typescript": "^4.5.5",
        "typescript-transform-paths": "^3.3.1",
        "webpack": "^5.67.0",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.7.3"
    },
    "scripts": {
        "task:typing": "tsm \"src/**/*.module.{css,scss}\"",
        "task:debug": "webpack --config ./config/webpack.config.js --mode=development --progress --watch",
        "task:build": "webpack --config ./config/webpack.config.js --mode=production --progress",
        "task:lint": "eslint --config ./config/eslint.config.js",
        "task:hooks": "husky install",
        "task:start-storybook": "start-storybook",
        "task:build-storybook": "build-storybook -o docs",
        "debug": "run-p \"task:typing -- --watch\" \"task:debug\"",
        "build": "run-s \"task:typing\" \"task:build\"",
        "lint": "run-s \"task:lint -- ./src/**/*.{js,ts,jsx,tsx}\"",
        "lint:fix": "run-s \"task:lint -- --fix ./src/**/*.{js,ts,jsx,tsx}\"",
        "lint:fix-one": "run-s \"task:lint -- --fix {1}\" --",
        "storybook": "run-s \"task:typing\" \"task:start-storybook\"",
        "storybook:production": "run-s \"task:typing\" \"task:build-storybook\"",
        "prepare": "run-s \"task:hooks\""
    }
}